  },
  "devDependencies": {
    "chai": "^4.2.0",
    "jest": "^24.9.0",
    "nyc": "^14.0.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.1.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.5.2",
    "@types/chai": "^4.1.6",
    "@types/jest": "^24.9.1"
