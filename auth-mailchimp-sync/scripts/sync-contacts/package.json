{
  "name": "@firebaseextensions/sync-contacts",
  "version": "0.1.11",
  "description": "Synchronises auth users with mail-chimp",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "github.com/firebase/extensions.git",
    "directory": "auth-mailchimp-sync/scripts/sync-contacts"
  },
  "scripts": {
